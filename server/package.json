    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node src/app.js",
    "dev": "nodemon src/app.js --ext js,jsx --ignore sessions",
    "createDB": "npx sequelize db:create",
    "mig": "npx sequelize db:migrate:undo:all && npx sequelize db:migrate",
    "seed": "npx sequelize db:seed:all",
    "remig": "npx sequelize db:migrate:undo:all && npx sequelize db:migrate && npx sequelize db:seed:all",
    "dropDB": "npx sequelize db:drop",
    "dbRebuild": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all"
  },
